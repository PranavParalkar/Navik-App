<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Background gradient -->
    <item>
        <shape android:shape="rectangle">
            <gradient
                android:startColor="#4FACFE"
                android:endColor="#00F2FE"
                android:angle="135" />
            <corners android:radius="16dp" />
        </shape>
    </item>
    
    <!-- Data bars -->
    <item android:bottom="30dp" android:left="25dp" android:width="15dp" android:height="30dp" android:gravity="bottom|start">
        <shape android:shape="rectangle">
            <solid android:color="#FFFFFF" />
            <corners android:topLeftRadius="4dp" android:topRightRadius="4dp" />
        </shape>
    </item>
    
    <item android:bottom="30dp" android:gravity="center_horizontal|bottom" android:width="15dp" android:height="45dp">
        <shape android:shape="rectangle">
            <solid android:color="#FFFFFF" />
            <corners android:topLeftRadius="4dp" android:topRightRadius="4dp" />
        </shape>
    </item>
    
    <item android:bottom="30dp" android:right="25dp" android:width="15dp" android:height="35dp" android:gravity="bottom|end">
        <shape android:shape="rectangle">
            <solid android:color="#FFFFFF" />
            <corners android:topLeftRadius="4dp" android:topRightRadius="4dp" />
        </shape>
    </item>
</layer-list>